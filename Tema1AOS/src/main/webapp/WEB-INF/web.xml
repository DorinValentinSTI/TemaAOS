<!DOCTYPE web-app PUBLIC

 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"

 "http://java.sun.com/dtd/web-app_2_3.dtd" >



<web-app>

	<display-name>Tema 1 AOS</display-name>
	
	<!-- Define servlet, named helloServlet -->
	<servlet>
		<servlet-name>PersonModelServlet</servlet-name>
		<servlet-class>vali.PersonModelServlet</servlet-class>
	</servlet>

	<!-- Defines the path to access this Servlet -->
	<servlet-mapping>
		<servlet-name>PersonModelServlet</servlet-name>
		<url-pattern>/PersonModelServlet/*</url-pattern>
	</servlet-mapping>

</web-app>